,KAAA,OAAA,CAAA,WAAA,GAAA,KAAA,CAAA,EAAA,CAAA;;AACA,WAAA,IAAA,KAAA,GAAA,CAAA,EAAA,KAAA,GAAA,KAAA,CAAA,MAAA,EAAA,KAAA,EAAA,EAAA;AACA,YAAA,KAAA,GAAA,CAAA,IAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,GAAA,KAAA,CAAA,KAAA,CAAA,CAAA,WAAA,EAAA;AACA;AACA;;AACA,UAAA,OAAA,GAAA,KAAA,CAAA,IAAA,CAAA,EAAA,CAAA;AACA,aAAA,OAAA;AACA,KAvCA;AAwCA,IAAA,oBAxCA,kCAwCA;AACA,UAAA,KAAA,GAAA,KAAA,OAAA,CAAA,WAAA,GAAA,KAAA,CAAA,EAAA,CAAA;;AACA,WAAA,IAAA,KAAA,GAAA,CAAA,EAAA,KAAA,GAAA,KAAA,CAAA,MAAA,EAAA,KAAA,EAAA,EAAA;AACA,YAAA,KAAA,GAAA,CAAA,IAAA,CAAA,EAAA;AACA,UAAA,KAAA,CAAA,KAAA,CAAA,GAAA,KAAA,CAAA,KAAA,CAAA,CAAA,WAAA,EAAA;AACA;AACA;;AACA,UAAA,OAAA,GAAA,KAAA,CAAA,IAAA,CAAA,EAAA,CAAA;AACA,aAAA,OAAA;AACA;AAjDA,GAZA;AA+DA,EAAA,QAAA,EAAA;AACA,IAAA,cAAA,EAAA,0BAAA;AACA,UAAA,KAAA,GAAA,KAAA,OAAA,CAAA,KAAA,CAAA,EAAA,CAAA;AACA,aAAA,KAAA,CAAA,MAAA;AACA;AAJA;AA/DA,CAAA","sourcesContent":["<template lang=\"html\">\n\n  <section class=\"codificador\">\n    <input v-model=\"message\" placeholder=\"Escriba un texto\">\n    <br/>\n    <div v-if='this.cantidadLetras>0'>\n      <p>Cantidad de letras: {{this.cantidadLetras}}</p>\n      <br/>\n      <p>Vocales cambiadas: {{this.cambiarVocales()}}</p>\n      <br/>\n      <p>Texto en mayúscula: {{this.cambiarMayusculas()}}</p>\n      <br/>\n      <p>Texto en mínuscula: {{this.cambiarMinusculas()}}</p>\n      <br/>\n      <p>Mayúscula intercalada: {{this.mayusculaIntercalada()}}</p>\n      <br/>\n      <p>Minúscula intercalada: {{this.minusculaIntercalada()}}</p>\n      <br/>\n      <p>Mayusculas intercalada: {{this.mayusculas}}</p>\n    </div>\n  </section>\n\n</template>\n\n<script lang=\"js\">\n\n  export default  {\n    name: 'codificador',\n    props: [],\n    mounted () {\n\n    },\n    data () {\n      return {\n        message:''\n\n      }\n    },\n    methods: {\n      cambiarVocales(){\n        let array = this.message.toLowerCase().split('');\n        for (let index = 0; index < array.length; index++) {\n          switch (array[index]) {\n            case 'a':\n              array[index] = 'u';\n              break;\n            case 'e':\n              array[index] = 'o';\n              break;\n            case 'o':\n              array[index] = 'e';\n              break;\n            case 'u':\n              array[index] = 'a';\n              break;\n            default:\n              break;\n          }\n        }\n        let palabra = array.join('');\n        return palabra;\n      },\n      cambiarMayusculas(){\n        return this.message.toUpperCase();\n      },\n      cambiarMinusculas(){\n        return this.message.toLowerCase();\n      },\n      mayusculaIntercalada(){\n        let array = this.message.toLowerCase().split('');\n        for (let index = 0; index < array.length; index++) {\n          if(index%2==0){\n            array[index] = array[index].toUpperCase();\n          }\n        }\n        let palabra = array.join('');\n        return palabra;\n      },\n      minusculaIntercalada(){\n        let array = this.message.toLowerCase().split('');\n        for (let index = 0; index < array.length; index++) {\n          if(index%2!=0){\n            array[index] = array[index].toUpperCase();\n          }\n        }\n        let palabra = array.join('');\n        return palabra;\n      }\n    },\n    computed: {\n      cantidadLetras: function(){\n        let array = this.message.split('');\n        return array.length;\n      }\n    }\n\n}\n\n\n</script>\n\n<style scoped lang=\"css\">\n  .codificador {\n\n  }\n</style>\n"],"sourceRoot":"src/components"}]}