ula intercalada: {{this.minusculaIntercalada()}}</p>\n      <br/>\n      <p>Mayusculas intercalada: {{this.mayusculas}}</p>\n    </div>\n  </section>\n\n</template>\n\n<script lang=\"js\">\n\n  export default  {\n    name: 'codificador',\n    props: [],\n    mounted () {\n\n    },\n    data () {\n      return {\n        message:''\n\n      }\n    },\n    methods: {\n      cambiarVocales(){\n        let array = this.message.toLowerCase().split('');\n        for (let index = 0; index < array.length; index++) {\n          switch (array[index]) {\n            case 'a':\n              array[index] = 'u';\n              break;\n            case 'e':\n              array[index] = 'o';\n              break;\n            case 'o':\n              array[index] = 'e';\n              break;\n            case 'u':\n              array[index] = 'a';\n              break;\n            default:\n              break;\n          }\n        }\n        let palabra = array.join('');\n        return palabra;\n      },\n      cambiarMayusculas(){\n        return this.message.toUpperCase();\n      },\n      cambiarMinusculas(){\n        return this.message.toLowerCase();\n      },\n      mayusculaIntercalada(){\n        let array = this.message.toLowerCase().split('');\n        for (let index = 0; index < array.length; index++) {\n          if(index%2==0){\n            array[index] = array[index].toUpperCase();\n          }\n        }\n        let palabra = array.join('');\n        return palabra;\n      },\n      minusculaIntercalada(){\n        let array = this.message.toLowerCase().split('');\n        for (let index = 0; index < array.length; index++) {\n          if(index%2!=0){\n            array[index] = array[index].toUpperCase();\n          }\n        }\n        let palabra = array.join('');\n        return palabra;\n      }\n    },\n    computed: {\n      cantidadLetras: function(){\n        let array = this.message.split('');\n        return array.length;\n      }\n    }\n\n}\n\n\n</script>\n\n<style scoped lang=\"css\">\n  .codificador {\n\n  }\n</style>\n"]}]}